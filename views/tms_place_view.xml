<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    	<record model='ir.ui.view' id='tms_place_form'>
            <field name='name'>tms.place.locatel</field>
            <field name='model'>tms.place</field>
            <field name='inherit_id' ref='tms.view_tms_place_form'/>
            <field name='type'>form</field>
            <field name='arch' type='xml'>
                <header position='inside'>
                    <button name="get_places" string="Get Places from Locatel" type="object" icon="gtk-refresh"/>
